<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="forgotPassword.ForgotPasswordController">
   <children>
      <AnchorPane prefHeight="661.0" prefWidth="500.0" style="-fx-background-color: white;">
         <children>
            <Label layoutX="182.0" layoutY="150.0" text="Recover your password here">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <ComboBox fx:id="sq" layoutX="96.0" layoutY="322.0" prefHeight="50.0" prefWidth="340.0" promptText="Security Question" />
            <Button layoutX="213.0" layoutY="503.0" mnemonicParsing="false" onMouseClicked="#recoverPassword" prefHeight="45.0" prefWidth="231.0" styleClass="login-button" stylesheets="@../LoginScreen/application.css" text="Recover Password">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Button>
            <TextField fx:id="accountno" layoutX="96.0" layoutY="241.0" prefHeight="50.0" prefWidth="340.0" promptText="Your Account Number">
               <font>
                  <Font size="18.0" />
               </font>
            </TextField>
            <TextField fx:id="ans" layoutX="96.0" layoutY="402.0" prefHeight="50.0" prefWidth="340.0" promptText="Answer">
               <font>
                  <Font size="18.0" />
               </font>
            </TextField>
            <Button layoutX="14.0" layoutY="18.0" mnemonicParsing="false" onMouseClicked="#backtoLogin" prefHeight="42.0" prefWidth="50.0" styleClass="back-button" stylesheets="@../LoginScreen/application.css" text="&lt;-">
               <font>
                  <Font name="System Bold" size="19.0" />
               </font>
            </Button>
         </children></AnchorPane>
      <Button layoutX="439.0" layoutY="13.0" mnemonicParsing="false" onMouseClicked="#closeApp" prefHeight="42.0" prefWidth="50.0" styleClass="close-button" stylesheets="@../LoginScreen/application.css" text="X">
         <font>
            <Font name="System Bold" size="19.0" />
         </font>
      </Button>
      <Label layoutX="64.0" layoutY="100.0" text="Forgot Password">
         <font>
            <Font name="Constantia Bold" size="47.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
